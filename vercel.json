{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js"
    }
  ],
  "env": {
    "TELEGRAM_TOKEN": "@telegram_token",
    "GEMINI_API_KEY": "@gemini_api_key",
    "BOT_URL": "@bot_url"
  }
} 